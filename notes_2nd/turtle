
import random
import turtle

turtle.color("#F417CF")
turtle.pensize(20)
turtle.fillcolor("blue")
turtle.begin_fill()
for x in range(4):
    turtle.forward(250)
    turtle.right(90)
turtle.end_fill()


turtle.penup()
turtle.goto(-100, 100)
turtle.pendown()

turtle.color("#F417CF")
turtle.pensize(20)
turtle.fillcolor("blue")
turtle.begin_fill()
for x in range(4):
    turtle.forward(250)
    turtle.right(90)
turtle.end_fill()


turtle.done()
